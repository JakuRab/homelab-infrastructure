# ==========================================
# User/Group Configuration
# ==========================================
# Run 'id' command on narsis to get your user's PUID and PGID
# Example: id athires
#   uid=1000(athires) gid=1000(athires) groups=...
PUID=1000
PGID=1000

# ==========================================
# General Settings
# ==========================================
# Timezone (must match your location for proper scheduling)
TZ=Europe/Warsaw

# ==========================================
# FlareSolverr Configuration (Optional)
# ==========================================
# Log level: info, debug, warning, error
LOG_LEVEL=info

# Log HTML responses (useful for debugging, creates large logs)
LOG_HTML=false

# Captcha solver: none, hcaptcha-solver, recaptcha-solver
CAPTCHA_SOLVER=none
