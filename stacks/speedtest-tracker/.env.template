# Speedtest Tracker Configuration

# Application key (Laravel) - generate with: docker run --rm lscr.io/linuxserver/speedtest-tracker php artisan key:generate --show
APP_KEY=

# Admin credentials
ADMIN_NAME=Kuba
ADMIN_EMAIL=
ADMIN_PASSWORD=

# Test schedule (cron format) - default: every 6 hours
SPEEDTEST_SCHEDULE=0 */6 * * *

# Prune old results (days, 0 = never)
PRUNE_RESULTS_OLDER_THAN=0

# Speedtest server IDs (comma-separated)
SPEEDTEST_SERVERS=3671,7200,23122
