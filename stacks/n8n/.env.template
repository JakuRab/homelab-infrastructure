# n8n Configuration
# Timezone
GENERIC_TIMEZONE=Europe/Warsaw

# Host configuration (used for webhooks and editor URL)
N8N_HOST=n8n.rabalski.eu
N8N_PROTOCOL=https
N8N_PORT=5678
N8N_EDITOR_BASE_URL=https://n8n.rabalski.eu
WEBHOOK_URL=https://n8n.rabalski.eu

# Security and performance
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
DB_SQLITE_POOL_SIZE=1
N8N_RUNNERS_ENABLED=true

# Optional: Encryption key for credentials
# N8N_ENCRYPTION_KEY=your_random_encryption_key_here
